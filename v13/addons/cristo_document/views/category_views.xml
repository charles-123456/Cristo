<?xml version="1.0" encoding="utf-8"?>
<odoo>

	<record id="view_cristo_dms_category_form" model="ir.ui.view">
			<field name="name">muk_dms_category.form.inherit</field>
			<field name="model">muk_dms.category</field>
			<field name="inherit_id" ref="muk_dms.view_dms_category_form" />
			<field name="arch" type="xml">
				<field name="name" position="attributes">
					<attribute name="placeholder">Confidential</attribute>
				</field>
				<xpath expr="//button[@name='toggle_active']" position="attributes">            
			        <attribute name="invisible">1</attribute>        
			    </xpath>
			</field>
		</record>

</odoo>